{
    "require": {
        "carlcs/craft-assetmetadata": "^3.0",
        "castiron/next-builds": "^1.0",
        "craftcms/aws-s3": "^1.2",
        "craftcms/cms": "3.7.59",
        "craftcms/contact-form": "2.5.1",
        "craftcms/contact-form-honeypot": "1.0.3",
        "craftcms/google-cloud": "^1.4",
        "craftcms/redactor": "2.10.10",
        "jamesedmonston/graphql-authentication": "^1.12",
        "rosas/universal-dam-integrator": "dev-main#v0.2.0-alpha",
        "rynpsc/craft-phone-number": "1.5.0",
        "sebastianlenz/linkfield": "2.0.*@dev",
        "spicyweb/craft-neo": "2.13.15",
        "verbb/super-table": "2.7.3",
        "vlucas/phpdotenv": "^3.4.0",
        "wrav/oembed": "1.3.18"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lsst-epo/craft3-universal-dam-integrator.git"
        },
        {
            "type": "path",
            "url": "plugins/nextbuilds"
        }
    ],
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/",
            "modules\\userregistrationmodule\\": "modules/userregistrationmodule/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4.24"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
