{
    "require": {
        "castiron/next-builds": "^1.0",
        "craftcms/aws-s3": "^2.0.1",
        "craftcms/cms": "^4.0.0",
        "craftcms/contact-form": "^3.0.0",
        "craftcms/contact-form-honeypot": "^2.0.0",
        "craftcms/google-cloud": "^2.0.0",
        "craftcms/redactor": "^3.0.2",
        "jamesedmonston/graphql-authentication": "^2.0.0",
        "rosas/universal-dam-integrator": "dev-EPO-7560",
        "rynpsc/craft-phone-number": "^2.1.0",
        "sebastianlenz/linkfield": "^2.1.4",
        "spicyweb/craft-neo": "^3.3.6",
        "verbb/super-table": "^3.0.0",
        "vlucas/phpdotenv": "^3.4.0",
        "wrav/oembed": "^2.2.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lsst-epo/craft3-universal-dam-integrator.git"
        },
        {
            "type": "path",
            "url": "plugins/nextbuilds"
        }
    ],
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.5"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/",
            "modules\\userregistrationmodule\\": "modules/userregistrationmodule/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1.10"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
